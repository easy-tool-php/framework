    "keywords": [
        "frame",
        "php",
        "composer",
        "psr-12"
    ],
    "homepage": "https://easy-tool-php.github.io",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruce Zeng",
            "email": "zengliwei@163.com",
            "homepage": "https://zengliwei.github.io"
        }
    ],
    "require": {
        "laminas/laminas-code": "*",
        "symfony/console": "*",
        "psr/log": "*",
        "psr/http-message": "*",
        "psr/container": "*",
        "psr/link": "*",
        "psr/event-dispatcher": "*",
        "psr/http-server-handler": "*",
        "psr/simple-cache": "*",
        "psr/http-factory": "*",
        "psr/http-client": "*"
